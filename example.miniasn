bit16 ::= BITSTRING_16
uint8 ::= UINT_8

b::=CHOICE[a]
{
    UINT(a>0 AND a < 100)
    BOOL(a == 0)
    bit16(a == 100 OR a == 110)
    BITSTRING(DEFAULT)
}

arr::=ARRAY[g s]
{
    arr b[g]
    c b[3]
}

sss::= SEQUENCE[x f e ff] {
    a UINT_9
    g b[a]
}

MojaSekwencjaSeq::= SEQUENCE {
    a uint8
    b sss[a 1 1]
}